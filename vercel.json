{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4",
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/(robots.txt|favicon.ico)$", "dest": "/public/$1" },
    { "src": "/(css|js|img|images|fonts|build)/(.*)", "dest": "/public/$1/$2" },
    { "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico))", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}